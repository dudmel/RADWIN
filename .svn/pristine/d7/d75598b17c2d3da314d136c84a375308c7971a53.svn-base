<div>
    <h3 class="box-title">Radio</h3>
    <div>
        <div>
            <table class="table radio-table">
                <tbody>
                    <tr>
                        <td colspan="2">
                            <div class="radio-table-icon">
                                <i class="rad-sprite globe"></i>
                            </div>
                            <div class="quick-look-header">
                                Lat.    Long.       Hgt.
                            </div>
                            <div class="quick-look-text">
                                N/A, N/A, N/A
                            </div>
                        </td>
                    </tr>
                </tbody>
                <tbody class="radio-table-body">
                    <tr>
                        <td>Sector ID</td>
                        <td>{{ radio?.sectorId }}</td>
                    </tr>
                    <tr>
                        <td>Frequency</td>
                        <td>{{ radio?.currentFrequency | units: 'ghz' }}</td>
                    </tr>
                    <tr>
                        <td>Band</td>
                        <td>{{ radio?.currentBand }}</td>
                    </tr>
                    <tr>
                        <td>Channel BW</td>
                        <td>{{ radio?.currentCbw | units: 'mhz' }}</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>