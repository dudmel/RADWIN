.parent {
  display: flex;
  height: auto;
  flex-flow: row wrap;
  justify-content: inherit;
}

.child {
  width: auto;
  height: auto;
  border-right: 1px solid #f4f4f4;
  padding-right: 20px;
  padding-left: 20px;
  align-self: center;
}

.hsu_component
{
	display:flex; 
	flex-direction:row;
	margin-left: 20px; 
	margin-right: 20px; 
}
.hsu_side_bar
{
	//background-color : #72cd9a;
	width:4px; 
	border-top-left-radius: 4px; 
	border-bottom-left-radius: 4px; 
}
.hsu_side_bar.hbs
{
	background-color :#c6c7ca
}

.hsu_header
{
	height : 60px; 
	display: flex;
	flex-wrap: wrap;
	flex-direction : row;
	align-items : center;
	background-color : white;
}

.hsu_title
{
	color : black;	
	//font-size : 2em;
	//font-family: "arial";
	padding-left : 10px;
}

.hsu_table_row
{
	display:flex;
	flex-direction:row;
	flex-wrap : no-wrap;
}
.hsu_title_text
{
	font-family: "arial";
	padding-top : 5px;
	color : #a4acb4;
	font-size : 0.9em;
}

.pipe{
	padding-left : 10px;
	padding-right : 10px;
	color : orange;
	font-size : 0.9em;
	font-style : bold;
	display:block;
}



.hsu_table {
    display: flex;
    flex-direction : column;
	flex-wrap : wrap;
	border-bottom : 1px solid #d8d8d8;
}

.info_box
{
	height : 60px; 
	background-color : white;
	width : 210px;
	border-top : 1px solid #d8d8d8;
	padding-left : 20px;

}
.info_box.with_border
{
	border-right : 1px solid #d8d8d8;
}

.info_box_caption 
{
	color : #a4acb4;
	font-size : 0.8em;
	font-family: arial;
	padding-top : 10px;
}
.info_box_text
{
	color : #4f535e;
	font-family: arial;
	font-size : 1em;
	font-style : bold;
	padding-top : 3px;
}
.category_caption
{
	font-size : 1.5em;
	font-family : arial;
	color : #ef4023;
	padding-left : 20px;
	padding-top:10px;
	padding-bottom : 10px;
}