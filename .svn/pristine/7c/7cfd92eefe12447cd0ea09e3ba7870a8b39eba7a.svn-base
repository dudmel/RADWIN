 * ----------------
 */

.table {
  > {
    thead > tr > th, tbody > tr > th, tfoot > tr > th, thead > tr > td, tbody > tr > td, tfoot > tr > td {
      border-top: 1px solid #f4f4f4;
    }
    thead > tr > th {
      border-bottom: 2px solid #f4f4f4;
    }
  }
  tr td .progress {
    // margin-top: 5px;
  }
}

.table-bordered {
  border: 1px solid #f4f4f4;
  > {
    thead > tr > th, tbody > tr > th, tfoot > tr > th, thead > tr > td, tbody > tr > td, tfoot > tr > td {
      border: 1px solid #f4f4f4;
    }
    thead > tr > {
      th, td {
        border-bottom-width: 2px;
      }
    }
  }
}

.table.no-border {
  border: 0;
  td, th {
    border: 0;
  }
}

/* .text-center in tables */

table.text-center {
  text-align: center;
  td, th {
    text-align: center;
  }
}

.table.align {
  th {
    text-align: left;
  }
  td {
    text-align: right;
  }
}

/*
 * Component: Label
 * ----------------
 */

.label-default {
  background-color: #d2d6de;
  color: #444;
}


/*
 * Component: Users List
 * ---------------------
 */

.users-list > li {
  width: 25%;
  float: left;
  padding: 10px;
  text-align: center;
  img {
    border-radius: 50%;
    max-width: 100%;
    height: auto;
  }
  > a:hover {
    color: #999;
    .users-list-name {
      color: #999;
    }
  }
}

.users-list-name, .users-list-date {
  display: block;
}

.users-list-name {
  font-weight: 600;
  color: #444;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.users-list-date {
  color: #999;
  font-size: 12px;
}


/*
 * Component: modal
 * ----------------
 */

.modal {
  //background: rgba(0, 0, 0, 0.3);
}

.modal-content {
  -webkit-box-shadow: 0 2px 3px rgba(0, 0, 0, 0.125);
  box-shadow: 0 2px 3px rgba(0, 0, 0, 0.125);
  border: 0;
  -webkit-border-radius: 4px;
  border-radius: 4px;
}

@media (min-width: 768px) {
  .modal-content {
    -webkit-box-shadow: 0 2px 3px rgba(0, 0, 0, 0.125);
    box-shadow: 0 2px 3px rgba(0, 0, 0, 0.125);
  }
}

.modal-header {
  color: #ef4023;
  font-size: 18px;
  background: #f7f7f7;
  border-bottom: none;
  -webkit-border-radius: 4px;
  border-radius: 4px;
}

.modal-body {
  font-size: 16px;
}

.modal-footer {
  border-top: none;
  margin: auto;
  width: 34%;
  text-align: center;
}

.modal-header .close {
    opacity: 0.5;
    font-weight: 200;
    font-size: xx-large;
}

/*
 * General: Miscellaneous
 * ----------------------
 */

.pad {
  padding: 10px;
}

.margin {
  margin: 10px;
}

.margin-bottom {
  margin-bottom: 20px;
}

.margin-bottom-none {
  margin-bottom: 0;
}

.margin-r-5 {
  margin-right: 5px;
}

.inline {
  display: inline;
}

.description-block {
  display: block;
  margin: 10px 0;
  text-align: center;
  &.margin-bottom {
    margin-bottom: 25px;
  }
  > {
    .description-header {
      margin: 0;
      padding: 0;
      font-weight: 600;
      font-size: 16px;
    }
    .description-text {
      text-transform: uppercase;
    }
  }
}

.bg-red, .bg-yellow, .bg-aqua, .bg-blue, .bg-light-blue, .bg-green, .bg-navy, .bg-teal, .bg-olive, .bg-lime, .bg-orange, .bg-fuchsia, .bg-purple, .bg-maroon, .bg-black, .bg-red-active, .bg-yellow-active, .bg-aqua-active, .bg-blue-active, .bg-light-blue-active, .bg-green-active, .bg-navy-active, .bg-teal-active, .bg-olive-active, .bg-lime-active, .bg-orange-active, .bg-fuchsia-active, .bg-purple-active, .bg-maroon-active, .bg-black-active {
  color: #fff !important;
}

.callout {
  &.callout-danger, &.callout-warning, &.callout-info, &.callout-success {
    // color: #fff !important;
  }
}

.quick-look-bar {
  background-image:url("/assets/icon/pattern.png");
  height: 62px;
}

.quick-look-block {
  display: block;
  margin: 10px 0;
  text-align: left;
  &.border-right {
      border-right: 1px solid #d8d8d8;
    }
}

.quick-look-header {
  color: #76828e;
}

.quick-look-text {
  color: #273238;
  font-size: 16px;
}

.quick-look-wrapper {
  border-bottom: 2px solid #d9dcdf;
  padding-bottom: 30px;
}

.link-muted {
  color: #7a869d;
