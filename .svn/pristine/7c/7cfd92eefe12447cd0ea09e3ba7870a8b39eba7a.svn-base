    color: #606c84;
  }
}

.link-black {
  color: #666;
  &:hover, &:focus {
    color: #999;
  }
}

.hide {
  display: none !important;
}

.no-border {
  border: 0 !important;
}

.no-padding {
  padding: 0 !important;
}

.no-margin {
  margin: 0 !important;
}

.no-shadow {
  box-shadow: none !important;
}

.chart {
  position: relative;
  overflow: hidden;
  width: 100%;
  svg, canvas {
    width: 100% !important;
  }
}


/*
 * rad-skin
 * ----------------------
 */


.rad-skin {
  .main-header {
    .navbar {
      background-color: #fff;
      .nav {
        > li > a {
          color: #76828e;
          border-left: 1px solid #e9e9e9;
          min-height: 59px;
          &:hover, &:active, &:focus {
            background: rgba(0, 0, 0, 0.1);
            color: #f6f6f6;
          }
        }
        .open > a {
          background: rgba(0, 0, 0, 0.1);
          color: #f6f6f6;
          &:hover, &:focus {
            background: rgba(0, 0, 0, 0.1);
            color: #f6f6f6;
          }
        }
        > .active > a {
          background: rgba(0, 0, 0, 0.1);
          color: #f6f6f6;
        }
      }
      .sidebar-toggle {
        color: #273238;
        // &:hover {
        //   color: #f6f6f6;
        //   background: rgba(0, 0, 0, 0.1);
        //   background-color: #d73925;
        // }
      }
    }
    .logo {
      background-color: blue;
      color: #ffffff;
      border-bottom: 0 solid transparent;
      // &:hover {
      //   background-color: #d33724;
      // }
    }
    li.user-header {
      background-color: #dd4b39;
    }
  }
  .content-header {
    background: transparent;
    border-top: 3px solid #f7f7f7;
  }
  .wrapper, .main-sidebar, .left-side {
     background-color: #222d32;
  }
  .main-sidebar, .left-side {
    background-color: #222d32;
    width: 173px;
  }
  .user-panel > .info {
    color: #fff;
    > a {
      color: #fff;
    }
  }
  .sidebar-menu > li {
    &.header {
      color: #4b646f;
      background: #1a2226;
    }
    > a {
      // border-left: 3px solid transparent;
    }
    &:hover > a, &.active > a {
      color: #ffffff;
      background: #1e282c;
      //height: 44px;
      //border-left-color: $red-color;
    }
    > .treeview-menu {
      margin: 0 1px;
      background: #2c3b41;
    }
  }
  .sidebar a {
    color: #b8c7ce;
    &:hover {
      text-decoration: none;
    }
  }
  .treeview-menu > li {
    > a {
      color: #8aa4af;
    }
    &.active > a, > a:hover {
      color: #ffffff;
    }
  }
  .sidebar-form {
    border-radius: 3px;
    border: 1px solid #374850;
    margin: 10px 10px;
    input[type="text"], .btn {
      box-shadow: none;
      background-color: #374850;
      border: 1px solid transparent;
      height: 35px;
      -webkit-transition: all 0.3s ease-in-out;
      -o-transition: all 0.3s ease-in-out;
      transition: all 0.3s ease-in-out;
    }
    input[type="text"] {
      color: #666;
      border-top-left-radius: 2px;
      border-top-right-radius: 0;
      border-bottom-right-radius: 0;
      border-bottom-left-radius: 2px;
      &:focus {
        background-color: #fff;
        color: #666;
        + .input-group-btn .btn {
          background-color: #fff;
          color: #666;
          border-left-color: #fff;
        }
      }
    }
    .btn {
      color: #999;
      border-top-left-radius: 0;
      border-top-right-radius: 2px;
      border-bottom-right-radius: 2px;
      border-bottom-left-radius: 0;
    }
  }
}

@media (max-width: 767px) {
  .rad-skin .main-header .navbar .dropdown-menu li {
    &.divider {
      background-color: rgba(255, 255, 255, 0.1);
    }
    a {
      color: #fff;
      &:hover {
        // background: $red-color;
      }
    }
  }
}