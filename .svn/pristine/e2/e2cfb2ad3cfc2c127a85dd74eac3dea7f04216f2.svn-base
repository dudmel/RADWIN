.radio-table-body {
    background-image: url("/assets/icon/pattern.png");
    background-repeat: initial;
    border-top: 1px solid #cfd3da;
}

.radio-table {
  > {
    thead > tr > th, tbody > tr > th, tfoot > tr > th, thead > tr > td, tbody > tr > td, tfoot > tr > td {
      border-top: 1px solid #cfd3da;
    }
  }
  tr td .progress {
    margin-top: 5px;
  }
}

.radio-table-icon {
    float: left;
    padding-right: 20px;
}